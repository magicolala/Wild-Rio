{% extends 'base.html.twig' %}

{% block body %}

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Espace utilisateur
        <small>Panneau de modifications</small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="{{ path('user_espace') }}"><i class="fa fa-dashboard"></i>Espace</a></li>
        <li class="active">Panneau de modification</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <!-- Small boxes (Stat box) -->
      <div class="col-md-12 titre_espace"><h1>Bienvenue sur votre espace utilisateur <b>{{ app.user.username }}</b></h1></div>
      <div class="row">
        <div class="col-md-3"></div>
        <div class="col-md-6 form_espace">
            {{ form_start(form) }}
                {{ form_row(form.userNom) }}
                {{ form_row(form.userPrenom) }}
                {{ form_row(form.userVille) }}
                {{ form_row(form.userFonction) }}
            {{ form_end(form) }}
          </p>
        </div>
        <div class="col-md-3"></div>


      </div>
      <!-- /.row -->
      <!-- Main row -->
     
    </section>
    <!-- /.content -->
  </div>


{% endblock body %}
  

